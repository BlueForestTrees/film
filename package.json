{
  "name": "film",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rimraf dist && babel src --out-dir dist",
    "dev": "node --version && cross-env PORT=8089 nodemon src/index.js --exec babel-node"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express-blueforest": "1.0.3",
    "express-validator": "5.3.0",
    "mongo-registry": "1.0.13"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "5.2.0",
    "nodemon": "1.18.3",
    "rimraf": "2.6.2"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": []
      }
    }
  }
}
